{
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@material/base": "^13.0.0",
    "@material/floating-label": "^13.0.0",
    "@material/textfield": "^13.0.0",
    "@mui/material": "^5.2.8",
    "css-concat": "^0.0.4",
    "dart-sass": "^1.25.0",
    "esbuild": "^0.14.11",
    "esbuild-sass-plugin": "^2.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "build": "esbuild app/app.jsx --bundle --outfile=dist/app.js --minify",
    "build:tail": "tailwindcss -i ./app/app.tail.css -o ./dist/app.tail.css --minify",
    "build:sass": "sass app/app.scss dist/app.css --load-path node_modules/ --style compressed",
    "concat:css": "cat dist/app.css dist/app.tail.css > dist/all.css",
    "clean": "del dist/app.tail.css dist/app.css dist/app.css.map",
    "build:app": "npm run build && npm run build:tail && npm run build:sass && npm run concat:css && npm run clean"
  },
  "devDependencies": {
    "sass": "^1.48.0",
    "sass-loader": "^12.4.0",
    "tailwindcss": "^3.0.13",
    "webpack": "^5.66.0"
  }
}
